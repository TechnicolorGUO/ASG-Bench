{
  "input_dir": "results/original",
  "output_dir": "results/processed",
  "systems": null,
  "categories": null,
  "auto_convert_markdown": true,
  "overwrite_original_json": true,
  
  "normalize_outline": true,
  "llm_calibration": false,
  "remove_empty_titles": true,
  "min_outline_items": 2,
  
  "normalize_content": true,
  "remove_short_sections": false,
  "min_section_words": 10,
  "calculate_section_stats": true,
  
  "normalize_references": true, 
  "keep_number": false,
  "llm_quality_check": true,
  "remove_duplicate_refs": true,
  
  "llm_api_base": null,
  "llm_api_key": null,
  "llm_model": "qwen/qwen3-30b-a3b",
  "llm_temperature": 0.0,
  "llm_enable_reasoning": false,
  
  "enable_quality_check": false,
  "min_references": 5,
  "min_total_content_length": 1000,
  
  "log_level": "INFO",
  "log_file": "data_processing.log"
